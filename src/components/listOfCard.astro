---
import type { Product } from '../utils/types'
import Card from './Card.astro'
interface Props {
  result: Product[]
}
const { result = [] } = Astro.props
---

{result.map(item => (
  <Card id={item.id} body={item.description} to={`item/${item.id}`} price={item.price} img={item.images[0]} rating={item.rating} title={item.title}/>
))}